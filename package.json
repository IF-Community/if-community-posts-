    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/node": "^20.12.11",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.1.0",
